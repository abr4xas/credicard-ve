{
    "name": "abr4xas/credicard-ve",
    "description": "Es una interfaz para interactuar con la API de CrediCard. Proporciona métodos para obtener información sobre tarjetas bancarias y realizar pagos utilizando tarjetas de crédito o débito. Esta clase es útil para aplicaciones que necesitan procesar pagos de manera segura y eficiente utilizando la API de CrediCard.",
    "keywords": [
        "abr4xas",
        "creditcard-ve"
    ],
    "homepage": "https://github.com/abr4xas/credicard-ve",
    "license": "MIT",
    "authors": [
        {
            "name": "Angel",
            "email": "bullgram@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9.2",
		"ext-openssl": "*"
	},
    "require-dev": {
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.3.0",
        "spatie/ray": "^1.41.2"
    },
    "autoload": {
        "psr-4": {
            "CrediCardVe\\CrediCardVe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrediCardVe\\CrediCardVe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
